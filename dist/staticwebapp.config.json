{
  "routes": [
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/*.js.map",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "Content-Type": "text/css"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".json": "application/json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon",
    ".map": "application/json"
  }
}
